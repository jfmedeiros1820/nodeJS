<html>
    <body>
        <h1>List of Products</h1>
        <table border="1px">
            <tr>
                <td>Id</td>
                <td>Title</td>
                <td>Description</td>
                <td>Price</td>
            </tr>
            <% for(var i=0; i<list.length; i++) {%>
                <tr>
                    <td><%= list[i].id %></td>
                    <td><%= list[i].title %></td>
                    <td><%= list[i].price %></td>
                    <td><%= list[i].description%></td>
                </tr>
            <% } %>
        </table>
    </body>
</html>